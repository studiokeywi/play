{
  "name": "@studiokeywi/play",
  "browser": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "esbuild src/index.ts --outfile=dist/index.js & tsc",
