  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "esbuild": "^0.17.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@studiokeywi/play-confetti": "*",
    "@studiokeywi/play-konami": "*"
  }
}
